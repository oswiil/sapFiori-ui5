<mvc:View xmlns:core="sap.ui.core"
  controllerName="msupply.project1.controller.countzones"
  xmlns:mvc="sap.ui.core.mvc"
  displayBlock="true"
  xmlns="sap.m"
>
  <Page id="page" title="{i18n>title}">
    <content>
      <Table id="table"
        items="{YourModel>/results}"
        mode="SingleSelectMaster"
      >
        <columns>
          <Column id="Column1">
            <Label text="ID" id="label1" />
          </Column>
          <Column id="Column2">
            <Label text="Name"   id="label2"/>
          </Column>
          <Column id="Column3">
            <Label text="Description"  id="label3" />
          </Column>
          <Column id="Column4">
            <Label text="PurchaseCenter"  id="label4" />
          </Column>
          <Column id="Column5">
            <Label text="Status"   id="label5"/>
          </Column>
        </columns>
        <items>
          <ColumnListItem  id="columlistItem">
            <cells>
              <Text text="{YourModel>ID}"   id="text1" />
              <Text text="{YourModel>Name}"    id="text2"/>
              <Text text="{YourModel>Description}"   id="text3" />
              <Text text="{YourModel>Center}"    id="text4"/>
              <Text text="{YourModel>Status}"   id="text5" />
            </cells>
          </ColumnListItem>
        </items>
      </Table>
      <Button id="finishbutton" text="AÃ±adir registro" press="onAddRecord" />

    </content>
  </Page>
</mvc:View>
